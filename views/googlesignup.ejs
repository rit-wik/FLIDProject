<%- include("partials/header"); -%>

      <div class="signup">
      <p class="signup-title">Sign Up</p>
      <form class="signup-form" action="/createaccountgoogleuser" method="post">
        <div class="form-row">
          <div class="col">
            <div class="form-group">
              <label for="profession" class="form-label">Profession:</label> <br>
              <input type="text" name="Profession" class="form-label" id="profession" required>
            </div>
            <div class="form-group">
              <label for="residence" class="form-label">State Of Residence:</label> <br>
              <input type="text" name="stateOfResidence" class="form-label" id="residence" required>
            </div>
            </div>
            <div class="col">
                <div class="form-group">
                  <label for="mobile" class="form-label">Mobile Number:</label> <br>
                  <input type="number" name="mobileNumber" class="form-label" id="mobile" required>
                </div>

                <div class="form-group">
                  <label for="city" class="form-label">City Of Residence:</label> <br>
                  <input type="text" name="cityOfResidence" class="form-label" id="city" required>
                </div>
            </div>
            <button type="submit" class="btn btn-primary btn-lg btn-signup-1" style="top:320px;">Create Account</button>
          </div>
      </form>
      </div>

  <%- include("partials/footer"); -%>
